<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="appBarType">
        <enum name="products" value="1" />
        <enum name="basket" value="2" />
        <enum name="basketNull" value="3" />
    </attr>

    <declare-styleable name="AppTabBar">
        <attr name="appBar_title" format="string"/>
        <attr name="appBarType" />
        <attr name="appBar_badge_count" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BasketStatus">
        <attr name="basket_sum" format="string"/>
    </declare-styleable>
</resources>